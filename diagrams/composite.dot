digraph {

include(common.style)

initial [ shape=point height=0.4 ];
initial_sub [ shape=point height=0.4 ];
final_sub [ shape=doublecircle label="" ];
A_sub [ label="A" ];
B_sub [ label="B" ];

subgraph cluster0 {
    label = "OtherMachine"
    style = rounded

    initial_sub -> A_sub [ label="evt1" ];
    initial_sub -> B_sub [ label="evt2" ];
    A_sub -> final_sub [ label="evt1" ];
    B_sub -> final_sub [ label="evt1" ];
  }

initial -> initial_sub [ label="evt1" lhead=cluster0 ];
final_sub -> A [ label="evt2" ltail=cluster0 ];

}
